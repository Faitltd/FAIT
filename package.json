{
  "name": "fait-coop",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "FAIT Co-op platform with dual bundler support (Vite and Webpack)",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:auth": "cypress run --spec 'cypress/e2e/auth/**/*.cy.ts'",
    "cypress:services": "cypress run --spec 'cypress/e2e/services/**/*.cy.ts'",
    "test:e2e": "start-server-and-test dev http://localhost:5173 cypress:run",
    "test:e2e:auth": "start-server-and-test dev http://localhost:5173 cypress:auth",
    "test:e2e:services": "start-server-and-test dev http://localhost:5173 cypress:services"
