{
  "name": "fait-coop",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "FAIT Co-op platform built with SvelteKit",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:oauth": "node scripts/test-oauth.js",
    "test:auth-credentials": "node scripts/test-auth-credentials.cjs",
    "test:auth-credentials:sh": "./test-auth-credentials.sh",
    "test:local-auth-credentials": "node scripts/test-local-auth-credentials.cjs",
    "test:auth-comprehensive": "./test-auth-comprehensive.sh",
    "db:backup": "node scripts/backup-database.js",
    "db:restore": "node scripts/restore-database.js",
    "db:migrate": "bash scripts/apply_migrations.sh",
    "utils:process-file": "node scripts/process-large-file.js",
    "utils:generate-csv": "node scripts/generate-sample-csv.js",
    "webpack:dev": "webpack serve --config webpack.dev.cjs",
    "webpack:build": "webpack --config webpack.prod.cjs",
    "webpack:analyze": "cross-env ANALYZE=true webpack --config webpack.prod.cjs",
    "webpack:test": "webpack --config webpack.test.cjs",
    "webpack:dashboard": "webpack-dashboard -- webpack serve --config webpack.dashboard.cjs",
    "webpack:perf": "webpack --config webpack.perf.js",
    "webpack:perf:analyze": "cross-env ANALYZE=true webpack --config webpack.perf.js",
    "webpack:pwa": "webpack --config webpack.pwa.cjs",
    "webpack:pwa:analyze": "cross-env ANALYZE=true webpack --config webpack.pwa.cjs",
    "perf:monitor": "node scripts/performance-monitor.js",
    "perf:test": "node scripts/performance-test.js",
    "perf:build": "npm run build:perf && npm run webpack:perf",
    "perf:analyze": "node scripts/analyze-bundle.js",
    "perf:analyze-deps": "node scripts/analyze-dependencies.js",
    "analyze:bundle": "./scripts/analyze-bundle.sh",
    "analyze:bundle:simple": "./scripts/simple-bundle-analysis.sh",
    "check:react-icons": "./scripts/check-react-icons.sh",
    "pwa:build": "npm run webpack:pwa",
    "cypress:fix": "./fix-cypress-conflict.sh",
    "cypress:open": "npm run cypress:fix && cypress open",
    "cypress:run": "npm run cypress:fix && cypress run",
    "cypress:headless": "./run-headless-tests.sh",
    "cypress:ci": "./run-ci-tests.sh",
    "cypress:report": "./run-headless-with-report.sh",
    "cypress:working": "./run-working-tests.sh",
    "cypress:all": "./scripts/run-cypress-tests.sh",
    "cypress:fixed": "./run-fixed-tests.sh",
    "test:e2e": "./run-tests-with-server.sh",
    "test:e2e:ci": "cross-env CI=true ./run-ci-tests.sh basic",
    "test:e2e:all": "./run-headless-tests.sh all",
    "test:app-versions": "./run-tests.sh",
    "test:auth": "./test-auth-only.sh",
    "test:local-auth": "./test-local-auth.sh",
    "test:direct-login": "./test-direct-login.sh",
    "test:emergency-login": "./test-emergency-login.sh",
    "test:super-emergency-login": "./test-super-emergency-login.sh",
    "test:warranty": "./scripts/run-warranty-tests.sh",
    "test:fixed-login": "npx cypress run --spec 'cypress/e2e/fixed-login-test.cy.js'",
    "setup": "node scripts/setup-and-test.js",
    "seed": "node scripts/seed-database.js",
    "test:puppeteer": "node scripts/test-application.js",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.6",
    "@googlemaps/markerclusterer": "^2.5.3",
    "@stripe/stripe-js": "^2.4.0",
    "@supabase/supabase-js": "^2.49.4",
    "chart.js": "^4.4.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.0",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "libphonenumber-js": "^1.12.7",
    "lucide-svelte": "^0.460.0",
    "stripe": "^18.0.0",
    "tailwind-merge": "^2.2.1",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@faker-js/faker": "^8.4.1",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/google.maps": "^3.55.4",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "autoprefixer": "^10.4.18",
    "cross-env": "^7.0.3",
    "cypress": "^12.17.4",
    "cypress-file-upload": "^5.0.8",
    "dotenv": "^16.5.0",
    "eslint": "^9.9.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.35",
    "puppeteer": "^21.10.0",
    "chalk": "^4.1.2",
    "supabase": "^2.20.12",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.8.2",
    "vite": "^5.0.0"
  }
}
