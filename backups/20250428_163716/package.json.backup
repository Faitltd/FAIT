{
  "name": "fait-coop",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "FAIT Co-op platform with dual bundler support (Vite and Webpack)",
  "scripts": {
    "dev": "vite",
    "dev:simple": "vite --mode simple",
    "build": "vite build",
    "build:simple": "vite build --mode simple",
    "preview": "vite preview",
    "lint": "eslint .",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:oauth": "node scripts/test-oauth.js",
    "test:auth-credentials": "node scripts/test-auth-credentials.cjs",
    "test:auth-credentials:sh": "./test-auth-credentials.sh",
    "test:local-auth-credentials": "node scripts/test-local-auth-credentials.cjs",
    "test:auth-comprehensive": "./test-auth-comprehensive.sh",
    "db:backup": "node scripts/backup-database.js",
    "db:restore": "node scripts/restore-database.js",
    "db:migrate": "bash scripts/apply_migrations.sh",
    "utils:process-file": "node scripts/process-large-file.js",
    "utils:generate-csv": "node scripts/generate-sample-csv.js",
    "webpack:dev": "webpack serve --config webpack.dev.cjs",
    "webpack:build": "webpack --config webpack.prod.cjs",
    "webpack:analyze": "cross-env ANALYZE=true webpack --config webpack.prod.cjs",
    "webpack:test": "webpack --config webpack.test.cjs",
    "webpack:dashboard": "webpack-dashboard -- webpack serve --config webpack.dashboard.cjs",
    "cypress:fix": "./fix-cypress-conflict.sh",
    "cypress:open": "npm run cypress:fix && cypress open",
    "cypress:run": "npm run cypress:fix && cypress run",
    "test:e2e": "./run-tests-with-server.sh",
    "test:auth": "./test-auth-only.sh",
    "test:local-auth": "./test-local-auth.sh",
    "test:direct-login": "./test-direct-login.sh",
    "test:emergency-login": "./test-emergency-login.sh",
    "test:super-emergency-login": "./test-super-emergency-login.sh"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.6",
    "@radix-ui/react-tabs": "^1.0.4",
    "@stripe/stripe-js": "^2.4.0",
    "@supabase/supabase-js": "^2.49.4",
    "@types/crypto-js": "^4.2.2",
    "@types/react-router-dom": "^5.3.3",
    "chart.js": "^4.4.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.0",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "libphonenumber-js": "^1.12.7",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.30.0",
    "stripe": "^18.0.0",
    "tailwind-merge": "^2.2.1",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@faker-js/faker": "^8.4.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.16",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/google.maps": "^3.55.4",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "clean-webpack-plugin": "^4.0.0",
    "compression-webpack-plugin": "^11.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "cypress": "^12.17.4",
    "cypress-file-upload": "^5.0.8",
    "dotenv": "^16.5.0",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.4.35",
    "postcss-loader": "^8.1.1",
    "react-refresh": "^0.17.0",
    "style-loader": "^4.0.0",
    "supabase": "^2.20.12",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.3.2",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.2",
    "vite": "^6.2.4",
    "webpack": "^5.99.7",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-dashboard": "^3.3.8",
    "webpack-dev-server": "^5.2.1",
    "webpack-merge": "^6.0.1"
  }
}
